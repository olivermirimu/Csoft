<!Doctype html>
<html lang="en">

<head>
   <meta charset="utf-8">
   <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
   <meta http-equiv="x-ua-compatible" content="ie=edge">
   <link rel="stylesheet" href="/style.css" media="all">
   <title>Log in or Sign up</title>
</head>

<body>
   <div id="page" class="site">
      <header id="masthead">
         <div class="site-identity">
            <div class="site-title">
               <h1 id="title"><a href=""> CSOFT </a></h1>
               <p id="tagline"> </p>
            </div>
         </div>
         <div class="navigation" id="main-nav">
            <nav class="top-nav">
               <ul class="">
                  <%for(item of nav){%>
                     <li><a href="<%=item.link%>" class=""><%=item.page%></a></li>
                  <%}%>
               </ul>
            </nav>
            <div class="dropdown-menu">
               <button class="dropdown-button"> Menu
                  <i class="fa fa-carret-down"> </i>
               </button>
               <nav class="dropdown-content">
                  <ul class="">
                  <%for(item of nav){%>
                     <li><a href="<%=item.link%>" class=""><%=item.page%></a></li>
                  <%}%>
                  </ul>
               </nav>
            </div>
            <!--   <a href="javascript:void(0);" class="icon" onclick="menufunction()">&#9776; </a>-->
         </div>
      </header>

      <main id="main-content" class="form-page">
         <section class="log-in forms">
            <div class="form-head">
               <h2>Client Log In</h2>
            </div>
            <div id="log_message"></div>
            <form method="post" action="/dummy" class="data-entry">
               <label for="client">Username: </label>
               <input type="text" name="client" />
               <br> <br>
               <label for="password">Password: </label>
               <input type="password" name="password" />
               <br><br>
               <input id="logbt" type="submit" value="Log in" class="btn" />
               <a  href="/signUp"><input id="registerbt" type="button" value="Register" class="btn"/></a>
               <br><br>
               <a href=""><small>Forgot password?</small></a>
            </form>
         </section>
      </main>
      <footer>
         <section class="social">
            <a href="">
               <div id="facebook" class="fa fa-facebook"><small>Facebook</small></div>
            </a>
            <a href="">
               <div id="twitter" class="fa fa-twitter"><small>Twitter</small></div>
            </a>
            <a href="">
               <div id="instagram" class="fa fa-instagram"><small>Instagram</small>
               </div>
            </a>
            <a href="">
               <div id="pinterest" class="fa fa-pinterest"><small>Pinterest</small>
               </div>
            </a>
         </section>
         <small>&copy;CSOFT <script>
               document.write(new Date().getFullYear());
            </script></small>
      </footer>
   </div>
</body>

</html>